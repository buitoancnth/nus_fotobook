<!DOCTYPE html>
<html>
  <head>
    <title><%= t('titel_app') %></title>
    <%= render 'layouts/head_include' %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container main-feed">
      <% flash.each do |message_type, message| %>
      <div class="alert alert-<%= message_type %>">
        <a href="#" class="close" data-dismiss="alert">&times;</a>
        <%= message %>
      </div>
    <% end %>
      <div class="row">
        <%= render 'layouts/left_body' %>
        <div class="col-sm-10 col-xs-12 photo_album">
          <ul class="nav nav-pills center-pills" role="tablist" >
            <li><%= link_to t('.photos'), photos_path %></li>
            <li><%= link_to t('.albums'), albums_path %></li>
          </ul>
          <%= yield %>
        </div>
        <%= render 'layouts/footer' %>
      </div>
    </div>
  </body>
</html>
