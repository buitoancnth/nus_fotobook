doctype html
html
  head
    title = t('title_app')
    = render 'layouts/head_include'
  body
    = render 'layouts/header'
    .container.main-feed
      - flash.each do |message_type, message|
        div class=("alert alert-#{message}")
          a.close href="#" data-dismiss="alert" &times;
          = message
      .row
        = render 'layouts/admin_left_body'
        .col-sm-10.col-xs-12.photo_album
          = yield
      = render 'layouts/footer'
